{
  "name": "mentor-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.ts",
    "dev": "nodemon --exec ts-node server.ts",
    "dev:ollama": "cross-env AI_PROVIDER=ollama nodemon --exec ts-node -r dotenv/config server.ts",
    "dev:claude": "cross-env AI_PROVIDER=claude nodemon --exec ts-node -r dotenv/config server.ts",
    "build": "tsc",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/AutoGeneratedMigration -d src/config/database.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/database.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "dependencies": {
    "@highlight-run/node": "^3.12.15",
    "ai": "^4.3.16",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "stripe": "^18.2.1",
    "swagger-ui-express": "^5.0.1",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.24",
    "ulid": "^3.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.2",
    "yamljs": "^0.3.0",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/moment": "^2.11.29",
